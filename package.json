{
  "name": "wechat-jssdk",
  "version": "4.0.1",
  "description": "Next-Generation WeChat JS-SDK integration with NodeJS",
  "main": "./lib/index.js",
  "engines": {
    "node": ">= 6.0"
  },
  "scripts": {
    "start": "DEBUG=wechat* node ./demo/index.js",
    "dev": "DEBUG=wechat* nodemon -w ./lib -w ./demo ./demo/index.js",
    "test": "_mocha --exit test/**/*.js",
    "coverage": "istanbul cover _mocha --report lcov -- -R spec --exit test/**/*.js",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "prod": "NODE_ENV=production webpack --progress --mode production",
    "build": "webpack --progress --mode development && npm run prod",
    "watch": "webpack --progress --mode development --watch",
    "prepublish": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "**/*.js": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JasonBoy/wechat-jssdk.git"
  },
  "keywords": [
    "wechat",
    "weixin",
    "node",
    "jssdk",
    "wechat share",
    "mini program"
  ],
  "author": "Jason Jiang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JasonBoy/wechat-jssdk/issues"
  },
  "homepage": "https://github.com/JasonBoy/wechat-jssdk#readme",
  "dependencies": {
    "date-fns": "1.29.0",
    "debug": "4.1.0",
    "lodash.isempty": "4.4.0",
    "mongoose": "5.3.15",
    "request": "2.88.0",
    "request-promise": "4.2.2",
    "xml2js": "0.4.19"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-preset-env": "1.7.0",
    "body-parser": "1.18.3",
    "chai": "4.2.0",
    "cookie-parser": "1.4.3",
    "coveralls": "3.0.2",
    "express": "4.16.4",
    "express-session": "1.15.6",
    "husky": "1.2.0",
    "istanbul": "0.4.5",
    "lint-staged": "8.1.0",
    "lowdb": "1.0.0",
    "mocha": "5.2.0",
    "mocha-lcov-reporter": "1.3.0",
    "nodemon": "1.18.7",
    "prettier": "1.15.3",
    "swig": "1.4.2",
    "webpack": "4.27.1",
    "webpack-cli": "3.1.2"
  }
}
